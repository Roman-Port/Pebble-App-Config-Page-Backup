<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>foursquare</ShortName>
  <Description>foursquare</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image height="16" width="16" type="image/x-icon">https://foursquare.com/favicon.ico</Image>
  <Url type="text/html" method="get" template="https://foursquare.com/explore">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <moz:SearchForm>https://foursquare.com/explore</moz:SearchForm>
</OpenSearchDescription>
